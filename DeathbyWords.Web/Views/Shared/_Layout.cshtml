<!DOCTYPE html>
<html>
<head class="mainHead">
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    
    @*JS imports*@
    <script src="~/Scripts/jquery-1.7.1.min.js" type="text/javascript"></script>
    <script src="~/Scripts/jquery-ui-1.8.20.min.js" type="text/javascript"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.min.js" type="text/javascript"></script>
    <script src="~/Scripts/jquery.validate.min.js" type="text/javascript"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js" type="text/javascript"></script>
    <script src="~/Scripts/knockout-2.1.0.js" type="text/javascript"></script>
    <script src="~/Scripts/modernizr-2.5.3.js" type="text/javascript"></script>
    <script src="~/lib/Plugins/jquery.backgroundpos.min.js" type="text/javascript"></script>
    <script src="~/Scripts/main.js" type="text/javascript"></script>
    <script src="~/lib/Plugins/akordeon/source/js/jquery.akordeon.js"></script>
    <script src="~/lib/Plugins/sebnitu-HorizontalNav-7f68f94/jquery.horizontalNav.js"></script>
    
    @*Favicon*@
    <link rel="icon" type="image/png" href="~/Images/favicon.jpg">
    
    @*CSS imports*@
    <link href="@Url.Content("~/Content/Site.css")" type="text/css" rel="stylesheet"/>
    
    @*Top navigation menu*@
    <nav class="horizontal-nav full-width horizontalNav-notprocessed">
        <ul>
            <li>@Html.ActionLink("Home", "Index", "Home")</li>
            <li>@Html.ActionLink("About", "About", "Home")</li>
            <li>@Html.ActionLink("Reading", "Events", "Book")</li>
            <li>@Html.ActionLink("Read", "Pastreads", "Book")</li>
            <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
        </ul>
    </nav>
</head>
<body class="mainBody">
    @RenderBody()
</body>
</html>
